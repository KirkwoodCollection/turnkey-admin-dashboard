# Admin Dashboard Production Environment Variables

# API Configuration
VITE_API_BASE_URL=https://api.turnkeyhms.com
VITE_WS_URL=wss://api.turnkeyhms.com/websocket/ws
VITE_WEBSOCKET_URL=wss://api.turnkeyhms.com/websocket/ws

# Firebase Configuration
VITE_FIREBASE_PROJECT_ID=turnkey-hms-prod
VITE_FIREBASE_API_KEY=your-firebase-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=turnkey-hms-prod.firebaseapp.com
VITE_FIREBASE_STORAGE_BUCKET=turnkey-hms-prod.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id-here
VITE_FIREBASE_APP_ID=your-app-id-here

# Analytics Configuration
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# App Configuration
VITE_APP_NAME=TurnkeyHMS Admin Dashboard
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DEBUG_MODE=false

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=true

# Performance
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_LAZY_LOADING=true
VITE_ENABLE_CODE_SPLITTING=true

# Monitoring
VITE_SENTRY_DSN=your-sentry-dsn-here
VITE_HOTJAR_ID=your-hotjar-id-here