{
  "name": "@turnkey/admin-dashboard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:production": "vite build --config vite.config.production.ts",
    "build:with-types": "tsc && vite build --config vite.config.production.ts",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:unit": "NODE_ENV=test jest --testPathPattern=unit",
    "test:integration": "NODE_ENV=test jest --testPathPattern=integration",
    "test:contract": "NODE_ENV=test jest --testPathPattern=contract",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:contract && npm run test:e2e",
    "test:ci": "NODE_ENV=test jest --ci --coverage --maxWorkers=2",
    "mock-server": "node tests/mocks/mockServer.js",
    "mock-server:start": "npm run mock-server & echo $! > mock-server.pid",
    "mock-server:stop": "kill $(cat mock-server.pid) && rm mock-server.pid",
    "pact:publish": "pact-broker publish pacts --consumer-app-version=$npm_package_version --broker-base-url=http://localhost:9292",
    "pact:verify": "pact-broker verify --provider=turnkey-analytics-api --broker-base-url=http://localhost:9292",
    "docker:build:prod": "docker build -f Dockerfile.production -t turnkey-admin-dashboard:latest .",
    "docker:run:prod": "docker run -p 8080:8080 turnkey-admin-dashboard:latest",
    "analyze:bundle": "npm run build:production && npx vite-bundle-analyzer dist/stats.html",
    "grafana:build": "./scripts/build-grafana.sh",
    "grafana:deploy": "./scripts/deploy-grafana.sh",
    "grafana:dev": "docker-compose -f docker-compose.grafana.yml up"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/lab": "^7.0.0-beta.17",
    "@mui/material": "^5.14.18",
    "@tanstack/react-query": "^5.8.4",
    "@types/cors": "^2.8.19",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "firebase": "^10.14.1",
    "plotly.js": "^2.27.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.20.0",
    "react-window": "^1.8.10",
    "recharts": "^2.15.4",
    "web-vitals": "^3.5.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@pact-foundation/pact": "^12.1.0",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.10",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/react-plotly.js": "^2.6.3",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.54.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-websocket-mock": "^2.5.0",
    "jsdom-global": "^3.0.2",
    "msw": "^2.0.11",
    "rollup-plugin-visualizer": "^5.9.2",
    "terser": "^5.24.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vite-plugin-compression": "^0.5.1"
  }
}
