{
  "dashboard": {
    "id": null,
    "uid": "turnkey-main",
    "title": "TurnkeyHMS Analytics Dashboard",
    "tags": ["analytics", "turnkey"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Active Sessions",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "datasource": {
              "type": "marcusolsson-json-datasource",
              "uid": "analytics-api"
            },
            "refId": "A",
            "method": "GET",
            "urlPath": "/metrics/realtime",
            "queryParams": "",
            "params": [
              ["property_id", "$property_id"]
            ],
            "fields": [
              {
                "jsonPath": "$.active_sessions",
                "name": "Active Sessions",
                "type": "number"
              }
            ],
            "cacheDurationSeconds": 5
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 200}
              ]
            },
            "color": {
              "mode": "thresholds"
            },
            "unit": "short"
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Conversion Rate",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "datasource": {
              "type": "marcusolsson-json-datasource",
              "uid": "analytics-api"
            },
            "refId": "A",
            "method": "GET",
            "urlPath": "/metrics/overview",
            "queryParams": "",
            "params": [
              ["property_id", "$property_id"],
              ["period", "24h"]
            ],
            "fields": [
              {
                "jsonPath": "$.kpis.conversion_rate",
                "name": "Conversion Rate",
                "type": "number"
              }
            ],
            "cacheDurationSeconds": 60
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "percentage",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 2}
              ]
            },
            "unit": "percentunit",
            "decimals": 2
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Revenue Today",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "datasource": {
              "type": "marcusolsson-json-datasource",
              "uid": "analytics-api"
            },
            "refId": "A",
            "method": "GET",
            "urlPath": "/metrics/overview",
            "queryParams": "",
            "params": [
              ["property_id", "$property_id"],
              ["period", "24h"]
            ],
            "fields": [
              {
                "jsonPath": "$.kpis.total_revenue",
                "name": "Revenue",
                "type": "number"
              }
            ],
            "cacheDurationSeconds": 60
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "unit": "currencyUSD",
            "decimals": 0
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "center"
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Occupancy Rate",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "datasource": {
              "type": "marcusolsson-json-datasource",
              "uid": "analytics-api"
            },
            "refId": "A",
            "method": "GET",
            "urlPath": "/metrics/overview",
            "queryParams": "",
            "params": [
              ["property_id", "$property_id"],
              ["period", "24h"]
            ],
            "fields": [
              {
                "jsonPath": "$.kpis.occupancy_rate",
                "name": "Occupancy",
                "type": "number"
              }
            ],
            "cacheDurationSeconds": 60
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "percentage",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "percentunit",
            "decimals": 1,
            "max": 1,
            "min": 0
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 5,
        "type": "bargauge",
        "title": "Booking Funnel",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "datasource": {
              "type": "marcusolsson-json-datasource",
              "uid": "analytics-api"
            },
            "refId": "A",
            "method": "GET",
            "urlPath": "/metrics/funnel",
            "queryParams": "",
            "params": [
              ["property_id", "$property_id"],
              ["period", "24h"]
            ],
            "fields": [
              {
                "jsonPath": "$.stages[*].name",
                "name": "Stage"
              },
              {
                "jsonPath": "$.stages[*].count",
                "name": "Count",
                "type": "number"
              }
            ],
            "cacheDurationSeconds": 300
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "blue", "value": null}
              ]
            },
            "unit": "short",
            "displayName": "${__field.labels.Stage}"
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true,
          "minVizWidth": 0,
          "minVizHeight": 10
        }
      },
      {
        "id": 6,
        "type": "table",
        "title": "Top Destinations",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "datasource": {
              "type": "marcusolsson-json-datasource",
              "uid": "analytics-api"
            },
            "refId": "A",
            "method": "GET",
            "urlPath": "/metrics/top",
            "queryParams": "",
            "params": [
              ["category", "destinations"],
              ["metric", "bookings"],
              ["limit", "10"],
              ["period", "7d"]
            ],
            "fields": [
              {
                "jsonPath": "$.items[*].rank",
                "name": "Rank",
                "type": "number"
              },
              {
                "jsonPath": "$.items[*].name",
                "name": "Destination"
              },
              {
                "jsonPath": "$.items[*].value",
                "name": "Bookings",
                "type": "number"
              },
              {
                "jsonPath": "$.items[*].percentage_of_total",
                "name": "% of Total",
                "type": "number"
              }
            ],
            "cacheDurationSeconds": 900
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "inspect": false
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "% of Total"},
              "properties": [
                {
                  "id": "unit",
                  "value": "percentunit"
                },
                {
                  "id": "decimals",
                  "value": 1
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": false,
              "displayName": "Rank"
            }
          ]
        }
      },
      {
        "id": 7,
        "type": "heatmap",
        "title": "Booking Heatmap (Destination Ã— Days Until Arrival)",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 12},
        "targets": [
          {
            "datasource": {
              "type": "grafana-bigquery-datasource",
              "uid": "bigquery-analytics"
            },
            "refId": "A",
            "rawSql": "SELECT \n  destination,\n  days_until_arrival,\n  SUM(booking_count) as bookings\nFROM `${GCP_PROJECT_ID}.analytics.booking_heatmap`\nWHERE date >= CURRENT_DATE() - 7\nGROUP BY destination, days_until_arrival\nORDER BY destination, days_until_arrival",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        },
        "options": {
          "calculate": false,
          "cellGap": 1,
          "cellValues": {
            "unit": "short",
            "decimals": 0
          },
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "scheme",
            "scale": "exponential",
            "scheme": "Oranges",
            "steps": 64
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "short"
          }
        }
      },
      {
        "id": 8,
        "type": "timeseries",
        "title": "Sessions Over Time",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "datasource": {
              "type": "grafana-bigquery-datasource",
              "uid": "bigquery-analytics"
            },
            "refId": "A",
            "rawSql": "SELECT \n  TIMESTAMP_TRUNC(timestamp, HOUR) as time,\n  COUNT(DISTINCT session_id) as sessions\nFROM `${GCP_PROJECT_ID}.analytics.booking_funnel`\nWHERE timestamp >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 24 HOUR)\n  AND property_id = '${property_id}'\nGROUP BY time\nORDER BY time",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "opacity",
              "spanNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "showLegend": true,
            "displayMode": "list",
            "placement": "bottom",
            "calcs": []
          }
        }
      },
      {
        "id": 9,
        "type": "gauge",
        "title": "System Health Score",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "datasource": {
              "type": "marcusolsson-json-datasource",
              "uid": "analytics-api"
            },
            "refId": "A",
            "method": "GET",
            "urlPath": "/metrics/realtime",
            "queryParams": "",
            "params": [],
            "fields": [
              {
                "jsonPath": "$.system_health_score",
                "name": "Health Score",
                "type": "number"
              }
            ],
            "cacheDurationSeconds": 10
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 80}
              ]
            },
            "max": 100,
            "min": 0,
            "unit": "percent"
          }
        },
        "options": {
          "orientation": "auto",
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      }
    ],
